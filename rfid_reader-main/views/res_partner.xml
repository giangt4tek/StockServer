<odoo>
    <record id="contact_view_inherit" model="ir.ui.view">
        <field name="name">res.partner.tree</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form" />
        <field name="arch" type="xml">
            <xpath expr="//sheet" position="before">
                <header>
                    <button name="test_write_data" type="object" string="Test Write Data" class="btn btn-primary" />
                    <button name="test_read_data" type="object" string="Test Read Data" class="btn btn-primary" />
                    <button name="test_read_tid_data" type="object" string="Test Read TID Data" class="btn btn-primary" />
                </header>
            </xpath>
            <xpath expr="//sheet/group/group/span[@name='address_name']" position="before">
                <field name="card_tid"/>
                <!-- <field name="money"/> -->
            </xpath>
        </field>
    </record>
</odoo>